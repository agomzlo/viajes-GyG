{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/add-city\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/add-country\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/add-interest-point\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/logout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/verify\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/send-city-postcard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/send-monument-postcard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/update-monument-visited\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/choose-user\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/add\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/([^/]+?)\\/add\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/([^/]+?)\\/city\\/([^/]+?)\\/add\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/([^/]+?)\\/city\\/([^/]+?)\\/monument\\/([^/]+?)\\/send-postcard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/([^/]+?)\\/city\\/([^/]+?)\\/monument\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/([^/]+?)\\/city\\/([^/]+?)\\/send-postcard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/([^/]+?)\\/city\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/country\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/([^/]+?)\\/([^/]+?)\\/([^/]+?)\\/([^/]+?)\\/([^/]+?)\\/postcard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}