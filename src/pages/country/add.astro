---
import Layout from "@/layouts/Layout.astro";
import AddCountry from "@/components/AddCountry";

const { user } = Astro.locals;
---
<Layout title="Agregar Pais">
    <main class="w-full h-full flex flex-col justify-center items-center md:grid md:place-content-center px-4">
        <AddCountry user={user} client:only />
    </main>
</Layout>